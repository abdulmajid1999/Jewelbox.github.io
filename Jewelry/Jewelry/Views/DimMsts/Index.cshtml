            <td>
                @Html.DisplayFor(modelItem => item.Dim_Crt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dim_Pcs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dim_Gm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dim_Size)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dim_Rate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dim_Amt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DimMstId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DimMstId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DimMstId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
