            <th>
                @Html.DisplayNameFor(model => model.dimQltyMst)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dimQltySubMst)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Crt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Pcs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Gm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Size)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Rate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dim_Amt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
